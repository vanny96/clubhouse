<h1>Sign in</h1>

<%=form_for(:session, url: signin_path) do |f|%>
  <%= f.label :email %><br>
  <%= f.email_field :email %><br>

  <%= f.label :password %><br>
  <%= f.password_field :password %><br>

  <%= f.label :remember_me %>
  <%= f.check_box :remember_me%><br>

  <%= f.submit value: "Log In" %>
<% end %>