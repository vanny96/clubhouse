<h1>Create Post</h1>

<%= form_for @post do |f| %>
  <ul>
    <% @post.errors.full_messages do |message| %>
      <%= message %>
    <% end%>
  </ul>

  <%= f.label :title %><br>
  <%= f.text_field :title %><br>

  <%= f.label :body %><br>
  <%= f.text_area :body%><br>

  <%= f.submit value: "Publish"%>
<% end %> 

